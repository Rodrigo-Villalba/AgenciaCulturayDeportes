<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#dde3e7"
    tools:context=".GerenciasActivity">



    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_marginTop="32dp"
        android:contentDescription="@string/all"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/logoagencia"
        tools:ignore="VectorDrawableCompat" />

    <Button
        android:id="@+id/culturaButton"
        android:layout_width="270dp"
        android:layout_height="100dp"
        android:layout_marginTop="80dp"
        android:gravity="center"
        android:text="CULTURA"
        android:background="@drawable/ic_buttom"
        android:textSize="30sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView2" />

    <Button
        android:id="@+id/deportesButton"
        android:layout_width="270dp"
        android:layout_height="100dp"
        android:layout_marginTop="10dp"
        android:gravity="center"
        android:text="DEPORTES"
        android:background="@drawable/ic_buttom"
        android:textSize="30sp"
        app:layout_constraintBottom_toTopOf="@+id/actividadFisicaButton"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/culturaButton" />

    <Button
        android:id="@+id/actividadFisicaButton"
        android:layout_width="270dp"
        android:layout_height="100dp"
        android:layout_marginTop="10dp"
        android:gravity="center"
        android:text="ACTIVIDAD FISICA"
        android:background="@drawable/ic_buttom"
        android:textSize="30sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/deportesButton" />



</androidx.constraintlayout.widget.ConstraintLayout>